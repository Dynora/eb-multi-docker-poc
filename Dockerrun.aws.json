{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "node-app",
      "host": {
        "sourcePath": "/var/app/current/node-app"
      }
    },
    {
      "name": "uwsgi-app",
      "host": {
        "sourcePath": "/var/app/current/uwsgi-app"
      }
    },
    {
      "name": "nginx-proxy-conf",
      "host": {
        "sourcePath": "/var/app/current/proxy/conf.d"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "node-app1",
      "image": "node:0.12",
      "essential": false,
      "memory": 128,
      "command": [
        "/bin/bash",
        "/usr/src/app/run.sh"
      ],
      "mountPoints": [
        {
          "sourceVolume": "node-app",
          "containerPath": "/usr/src/app"
        }
      ]
    },
    {
      "name": "node-app2",
      "image": "node:0.12",
      "essential": false,
      "memory": 128,
      "command": [
        "/bin/bash",
        "/usr/src/app/run_another.sh"
      ],
      "mountPoints": [
        {
          "sourceVolume": "node-app",
          "containerPath": "/usr/src/app"
        }
      ]
    },
    {
      "name": "uwsgi-app1",
      "image": "dynora/trusty-py-postgres95",
      "build": "uwsgi-app/Dockerfile",
      "essential": false,
      "memory": 128,
      "command": [
        "/bin/bash",
        "/usr/src/app/run.sh"
      ],
      "mountPoints": [
        {
          "sourceVolume": "uwsgi-app",
          "containerPath": "/usr/src/app"
        }
      ]
    },
    {
      "name": "nginx-proxy",
      "image": "nginx",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "node-app1",
        "node-app2",
        "uwsgi-app1"
      ],
      "mountPoints": [
        {
          "sourceVolume": "node-app",
          "containerPath": "/usr/src/app",
          "readOnly": true
        },
        {
          "sourceVolume": "awseb-logs-nginx-proxy",
          "containerPath": "/var/log/nginx"
        },
        {
          "sourceVolume": "nginx-proxy-conf",
          "containerPath": "/etc/nginx/conf.d",
          "readOnly": true
        }
      ]
    }
  ]
}